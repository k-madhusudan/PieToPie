<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_individual_goal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="helios.com.productivityghanta.IndividualGoal">

    <TextView
        android:text="GOAL NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="42dp"
        android:id="@+id/textView26" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Name"
        android:ems="10"

        android:layout_alignParentTop="true"
        android:layout_marginTop="29dp"
        android:id="@+id/individGoalName"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="ACTIVITY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/individGoalName"
        android:layout_alignLeft="@+id/textView26"
        android:layout_alignStart="@+id/textView26"
        android:layout_marginTop="25dp"
        android:id="@+id/textView27" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Name"
        android:ems="10"
        android:layout_below="@+id/individGoalName"
        android:layout_alignLeft="@+id/individGoalName"
        android:layout_alignStart="@+id/individGoalName"
        android:layout_marginTop="14dp"
        android:id="@+id/individGoalActivityName" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Name"
        android:ems="10"
        android:layout_marginTop="17dp"
        android:id="@+id/individGoalFromTo"
        android:layout_below="@+id/individGoalActivityName"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="SET"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:id="@+id/textView28"
        android:layout_below="@+id/individGoalFromTo"
        android:layout_alignLeft="@+id/textView27"
        android:layout_alignStart="@+id/textView27" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Name"
        android:ems="10"
        android:layout_alignLeft="@+id/individGoalActivityName"
        android:layout_alignStart="@+id/individGoalActivityName"
        android:id="@+id/individGoalTarget"
        android:layout_below="@+id/individGoalFromTo" />

    <TextView
        android:text="ACHIEVED"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/individGoalTarget"
        android:layout_alignLeft="@+id/textView28"
        android:layout_alignStart="@+id/textView28"
        android:layout_marginTop="30dp"
        android:id="@+id/textView29" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Name"
        android:ems="10"
        android:id="@+id/individGoalActual"
        android:layout_alignBaseline="@+id/textView29"
        android:layout_alignBottom="@+id/textView29"
        android:layout_alignLeft="@+id/individGoalTarget"
        android:layout_alignStart="@+id/individGoalTarget" />
</RelativeLayout>